package homework7;

public class Power implements Expression {
	Expression e1;
	Expression e2;
	public Power(Expression e1, Expression e2) {
		this.e1 = e1;
		this.e2 = e2;
	}
	
	public Polinom evaluate() {
		Polinom a = evaluate();
		Polinom b = evaluate();
		return null;
	}

	public int compare(Expression b) {
		Polinom a = evaluate();
		Polinom bb = evaluate();
		return a.compare(bb);
	}
	
	public String printExp() {
		return "(" + e1.printExp() + "^" + e2.printExp() + ")";
	}

}
